{
    "user": {
        "name": "user",
        "workflow": "user",
        "titleTemplate": "{{firstName}} {{lastName}}",
        "search": {
            "url": "https://test-amorphie-fact-user.burgan.com.tr/user/search",
            "defaultPageSize": 100,
            "defaultPageNumber": 0,
            "search": true,
            "filter": [
                {
                    "data": "tag",
                    "widget": "dropdown",
                    "type": "reference",
                    "title": {
                        "tr-TR": "Etiket",
                        "en-EN": "Tag"
                    }
                },
                {
                    "data": "resourceRole",
                    "widget": "dropdown",
                    "type": "reference",
                    "title": {
                        "tr-TR": "Rol",
                        "en-EN": "Role"
                    }
                },
                {
                    "data": "reference",
                    "widget": "textField",
                    "type": "query",
                    "title": {
                        "tr-TR": "Referans",
                        "en-EN": "Reference"
                    }
                }
            ],
            "columns": [
                {
                    "data": "reference",
                    "title": {
                        "tr-TR": "Referans",
                        "en-EN": "Reference"
                    },
                    "sort": 1
                },
                {
                    "data": "firstName",
                    "title": {
                        "tr-TR": "İsim",
                        "en-EN": "First Name"
                    },
                    "sort": 2
                },
                {
                    "data": "lastName",
                    "title": {
                        "tr-TR": "Soyisim",
                        "en-EN": "Last Name"
                    },
                    "sort": 3
                },
                {
                    "data": "tag",
                    "title": {
                        "tr-TR": "Etiketler",
                        "en-EN": "Tags"
                    },
                    "sort": 4
                }
            ]
        },
        "display": {
            "workflow": "user",
            "url": "https://test-amorphie-fact-user.burgan.com.tr/user/search/@id",
            "summaryTemplate": {
                "tr-TR": "amorphy-backoffice-user-display-summary-tr",
                "en-EN": "amorphy-backoffice-user-display-summary-en"
            },
            "detailTemplate": {
                "tr-TR": "amorphy-backoffice-user-detail-tr",
                "en-EN": "amorphy-backoffice-user-detail-tr"
            },
            "history": true,
            "stateManager": true,
            "runningWorkflows": true,
            "availableWorkflows": true,
            "tabs": [
                {
                    "type": "render",
                    "entity": "consent",
                    "order": 0,
                    "id": "$.id",
                    "template": {
                        "tr-TR": "amorphy-backoffice-user-consent-listview",
                        "en-EN": "amorphy-backoffice-user-consent-listview"
                    },
                    "title": {
                        "tr-TR": "Riza Detaylari",
                        "en-EN": "Consent Details"
                    }
                },
                {
                    "type": "search",
                    "entity": "tag",
                    "order": 1,
                    "id": "$.id",
                    "template": {
                        "tr-TR": "amorphy-backoffice-user-consent-listview",
                        "en-EN": "amorphy-backoffice-user-consent-listview"
                    },
                    "title": {
                        "tr-TR": "Etiketler",
                        "en-EN": "Tags"
                    }
                }
            ]
        },
        "delete": {
            "url": "https://test-amorphie-fact-user.burgan.com.tr/user/@id"
        }
    },
