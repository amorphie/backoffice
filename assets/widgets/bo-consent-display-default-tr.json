{
  "type": "list_view",
  "children": [
    {
      "type": "container",
      "args": {
        "margin": 5,
        "height": 75,
        "padding": 14,
        "decoration": {
          "border": {
            "style": "solid",
            "width": 1
          },
          "borderRadius": {
            "type": "all",
            "radius": 14
          }
        }
      },
      "child": {
       
            "type": "row",
            "args": {
              "mainAxisAlignment": "spaceBetween"
            },
            "children": [
              {
                "type": "padding",
                "args": {
                  "padding": {
                    "left": 14,
                    "right": 14
                  }
                },
                "child": {
                  "type": "column",
                  "args": {
                    "crossAxisAlignment": "start"
                  },
                  "children": [
                    {
                      "type": "text",
                      "args": {
                        "text": "{{consent.name}}",
                        "style": {
                          "fontSize": 15,
                          "fontWeight": "bold",
                          "color" : "#1d1f29"
                        }
                      }
                    },
                    {
                      "type": "sized_box",
                      "args": {
                        "height": 3
                      }
                    },
                    {
                      "type": "text",
                      "args": {
                        "text": "{{consent.description}}",
                        "style": {
                          "fontSize": 12,
                          "fontWeight": "w400",
                          "color": "#1d1f29"
                        }
                      }
                    }
                  ]
                }
              },
              {
                "type": "text",
                "args": {
                  "text": "{{consent.date}}",
                  "style": {
                    "fontSize": 13,
                    "fontWeight": "bold",
                    "color" : "#1d1f29"
                  }
                }
              }
            ]
          }
    }
  ]
}