{
  "type": "neo_detail_page",
  "args": {
    "summary": {
      "type": "neo_bo_summary_widget",
      "args": {
        "name": "Tag"
      }
    },
    "tabs": {
      "type": "neo_bo_tabview",
      "args": {
        "data": [
          {
            "title": "Details",
            "view": {
              "type": "list_view",
              "args": {
                "children": [
                  {
                    "type": "neo_bo_detail_item_widget",
                    "args": {
                      "title": "ID",
                      "text": "$.data.id",
                      "copyValue": true
                    }
                  },
                  {
                    "type": "neo_bo_detail_item_widget",
                    "args": {
                      "title": "URL",
                      "text": "$.data.url",
                      "copyValue": true
                    }
                  },
                  {
                    "type": "neo_bo_detail_item_widget",
                    "args": {
                      "title": "TTL",
                      "text": "$.data.ttl",
                      "copyValue": true
                    }
                  },
                  {
                    "type": "neo_bo_detail_item_widget",
                    "args": {
                      "title": "Name",
                      "text": "$.data.name",
                      "copyValue": true
                    }
                  }
                ]
              }
            }
          }
        ]
      }
    }
  }
}