{
    "type": "neo_detail_page",
    "args": {
        "summary": {
            "type": "neo_bo_summary_widget",
            "args": {
                "name": "Resource"
            }
        },
        "tabs": {
            "type": "neo_bo_tabview",
            "args": {
                "data": [
                    {
                        "title": "Tab1",
                        "view": {
                            "type": "list_view",
                            "args": {
                                "children": [
                                    {
                                        "type": "neo_bo_detail_item_widget",
                                        "args": {
                                            "title": "First Name",
                                            "text": "$.data.firstName",
                                            "copyValue": true
                                        }
                                    },
                                    {
                                        "type": "neo_bo_detail_item_widget",
                                        "args": {
                                            "title": "Last Name",
                                            "text": "$.data.lastName",
                                            "copyValue": true
                                        }
                                    },
                                    {
                                        "type": "neo_bo_detail_item_widget",
                                        "args": {
                                            "title": "Reference",
                                            "text": "$.data.reference",
                                            "copyValue": true
                                        }
                                    },
                                    {
                                        "type": "neo_bo_detail_item_widget",
                                        "args": {
                                            "title": "E-Mail",
                                            "text": "$.data.eMail",
                                            "copyValue": true
                                        }
                                    },
                                    {
                                        "type": "neo_bo_detail_item_widget",
                                        "args": {
                                            "title": "Created At",
                                            "text": "$.createdAt",
                                            "copyValue": true
                                        }
                                    }
                                ]
                            }
                        }
                    }
                ]
            }
        }
    }
}